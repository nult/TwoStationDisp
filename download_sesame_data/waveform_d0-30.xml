<sod>
    <properties>
        <eventLag>
            <unit>HOUR</unit>
            <value>0</value>
        </eventLag>
        <eventChannelPairProcessing>noCheck</eventChannelPairProcessing>
    </properties>
    <eventArm>
        <fdsnEvent>
            <pointDistance>
                <unit>DEGREE</unit>
                <min>20</min>
                <max>90</max>
                <latitude>32</latitude>
                <longitude>-83</longitude>
            </pointDistance>
            <originDepthRange>
                <unit>KILOMETER</unit>
                <min>0</min>
                <max>30</max>
            </originDepthRange>
            <originTimeRange>
                <startTime>
                    2013-08-12T00:00:00.000Z
                </startTime>
                <endTime>
                    2013-08-12T23:59:59.999Z
                </endTime>
            </originTimeRange>
            <magnitudeRange>
                <min>5.0</min>
                <max>10</max>
            </magnitudeRange>
        </fdsnEvent>
        <removeEventDuplicate/>
        <printlineEventProcess>
            <template>$event.getLongitude('##0.0000;-##0.0000') $event.getLatitude('##0.0000;-##0.0000') $event.getDepth('###0.##') ${event.getTime('yyyy_MM_dd_HH_mm_ss_SSS')} $event.magnitudeValue$event.magnitudeType</template>
        </printlineEventProcess>
    </eventArm>
    <networkArm>
        <fdsnStation>
            <networkCode>Z9</networkCode>
            <stationBoxArea>
                <latitudeRange>
                    <min>28.5</min>
                    <max>36</max>
                </latitudeRange>
                <longitudeRange>
                    <min>-85</min>
                    <max>-81</max>
                </longitudeRange>
            </stationBoxArea>
        </fdsnStation>
        <networkOR>
            <networkCode>Z9</networkCode>
        </networkOR>
        <stationAND>
            <stationBoxArea>
                <latitudeRange>
                    <min>28.5</min>
                    <max>36</max>
                </latitudeRange>
                <longitudeRange>
                    <min>-85</min>
                    <max>-81</max>
                </longitudeRange>
            </stationBoxArea>
            <printlineStationProcess>
                <template>$station.getLongitude(' ##0.0000;-##0.0000') $station.getLatitude(' ##0.0000;-##0.0000') $station.getElevation('###0.') ${station.netCode}.${station.code}</template>
            </printlineStationProcess>
        </stationAND>
    </networkArm>
    <waveformVectorArm>
        <phaseRequest>
            <beginPhase>ttp</beginPhase>
            <beginOffset>
                <unit>MINUTE</unit>
                <value>0</value>
            </beginOffset>
            <endPhase>ttp</endPhase>
            <endOffset>
                <unit>MINUTE</unit>
                <value>60</value>
            </endOffset>
        </phaseRequest>
        <fullCoverage/>
        <printlineSeismogramProcess/>
        <sacWriter/>
        <responseGain/>
        <rMean/>
        <rTrend/>
        <integrate/>
                <sampleSyncronize/>
                <vectorTrim/>
                <rotateGCP/>
        <sacWriter>
            <workingDir>processedSeismograms1</workingDir>
        </sacWriter>
        <legacyExecute>
            <command>echo Sod saved this file</command>
        </legacyExecute>

    </waveformVectorArm>
</sod>
